{
  "name": "portfolio",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite-ssg build",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint --ext .ts,.vue .",
    "check:stylelint": "stylelint **/*.{scss,vue}",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "pnpm check:prettier && pnpm check:eslint && pnpm check:stylelint && pnpm check:ts",
    "fix:prettier": "prettier --loglevel warn --write .",
    "fix:eslint": "pnpm check:eslint --fix",
    "fix:stylelint": "pnpm check:stylelint --fix",
    "fix:all": "pnpm fix:prettier && pnpm fix:eslint && pnpm fix:stylelint"
  },
  "dependencies": {
    "@vueuse/core": "10.5.0",
    "pinia": "2.1.7",
    "vue": "3.3.7",
    "vue-i18n": "9.6.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "1.4.0",
    "@types/node": "20.8.9",
    "@typescript-eslint/parser": "6.9.0",
    "@unhead/vue": "1.7.4",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/tsconfig": "0.4.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-vue": "9.18.1",
    "postcss-html": "1.5.0",
    "prettier": "3.0.3",
    "sass": "1.69.5",
    "stylelint": "15.11.0",
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-standard-scss": "11.0.0",
    "typescript": "5.2.2",
    "unplugin-auto-import": "0.16.7",
    "vite": "4.5.0",
    "vite-plugin-pages": "0.31.0",
    "vite-plugin-vue-layouts": "0.8.0",
    "vite-ssg": "0.23.4",
    "vite-ssg-sitemap": "0.6.0",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.22"
  }
}
